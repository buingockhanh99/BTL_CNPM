@model IEnumerable<THUVIENSO.Models.account>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutKhachHang1.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.accountname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.passwords)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.levels)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.customer.username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.accountname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.passwords)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.levels)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.customer.username)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
